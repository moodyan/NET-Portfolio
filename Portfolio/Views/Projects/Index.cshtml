@{
    ViewData["Title"] = "Projects";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script>
    $(document).ready(function () {
        $.ajax({
            url: '@Url.Action("GetStarredRepos", "Projects")',
            type: "GET",
            success: function (result) {
                $('#result').html(result);
            }
        })
    });
</script>
<div id="project-header">
    <h2>PROJECTS</h2>
</div>

<div id="result"> 
</div>